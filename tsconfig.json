{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "moduleResolution": "Node",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2015"
    ],
    "esModuleInterop": true,
    "allowJs": false,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "@types/vuelidate",
      "@nuxt/types",
      "@nuxtjs/axios",
      "@nuxtjs/toast",
      "@nuxtjs/vuetify",
      "./node_modules/@types/"
    ],
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
